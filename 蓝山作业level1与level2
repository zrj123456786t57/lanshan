蓝山level1
package main

import "fmt"

func addUp(x int, y int) int {
	return x + y
}

func main() {
	result := addUp(4, 4)
	fmt.Println("结果是", result)
}
蓝山level2
package main

import (
	"fmt"
	"math"
)

// calculateCircleArea
func calculateCircleArea(radius float64) float64 {
	return math.Pi * math.Pow(radius, 2)
}
func main() {
	var radius float64 = 5.0
	fmt.Printf("圆的面积为：%.2f\n", calculateCircleArea(radius))
}
